# MongoDB Connection String
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database

# Vercel Blob Storage Token (for your app's files)
BLOB_READ_WRITE_TOKEN=vercel_blob_rw_xxxxx

# Vercel Blob Storage Token for BACKUPS (create a separate blob store in Vercel)
BACKUP_BLOB_TOKEN=vercel_blob_rw_xxxxx

# Secret to protect the cron endpoint (generate a random string)
CRON_SECRET=your-random-secret-string-here
